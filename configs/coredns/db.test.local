$ORIGIN test.local.
$TTL 60

@       IN      SOA     ns.test.local. admin.test.local. (
                        2024102401 ; serial
                        3600       ; refresh
                        1800       ; retry
                        604800     ; expire
                        60 )       ; minimum TTL

; Nameserver
@       IN      NS      ns.test.local.
ns      IN      A       172.28.0.53

; Wildcard points to Traefik on infra network
*       IN      A       172.28.0.5
